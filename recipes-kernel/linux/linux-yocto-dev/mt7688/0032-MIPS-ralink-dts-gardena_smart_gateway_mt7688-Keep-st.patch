From d0e68297e4fd4fb972ab983eeb5edc3b33bf7b1d Mon Sep 17 00:00:00 2001
From: Reto Schneider <code@reto-schneider.ch>
Date: Wed, 2 Jan 2019 02:16:56 +0000
Subject: [PATCH] MIPS: ralink: dts: gardena_smart_gateway_mt7688: Keep state
 of all LED

---
 .../dts/ralink/gardena_smart_gateway_mt7688.dts    | 22 +++++++++++-----------
 1 file changed, 11 insertions(+), 11 deletions(-)

diff --git a/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts b/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
index 89ee57d78fa8..aa72dfab5d84 100644
--- a/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
+++ b/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
@@ -20,67 +20,67 @@
 		power_blue {
 			label = "smartgw:power:blue";
 			gpios = <&gpio0 18 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		power_green {
 			label = "smartgw:power:green";
 			gpios = <&gpio0 19 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		power_red {
 			label = "smartgw:power:red";
 			gpios = <&gpio0 22 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		radio_blue {
 			label = "smartgw:radio:blue";
 			gpios = <&gpio0 23 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		radio_green {
 			label = "smartgw:radio:green";
 			gpios = <&gpio0 24 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		radio_red {
 			label = "smartgw:radio:red";
 			gpios = <&gpio0 25 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		internet_blue {
 			label = "smartgw:internet:blue";
 			gpios = <&gpio0 26 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		internet_green {
 			label = "smartgw:internet:green";
 			gpios = <&gpio0 27 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		internet_red {
 			label = "smartgw:internet:red";
 			gpios = <&gpio0 28 GPIO_ACTIVE_HIGH>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		ethernet_link {
 			label = "smartgw:eth:link";
 			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
-			default-state = "off";
+			default-state = "keep";
 		};
 
 		ethernet_activity {
 			label = "smartgw:eth:act";
 			gpios = <&gpio1 11 GPIO_ACTIVE_LOW>;
-			default-state = "off";
+			default-state = "keep";
 		};
 	};
 
