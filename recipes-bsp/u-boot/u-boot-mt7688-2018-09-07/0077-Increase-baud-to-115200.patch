+++ b/configs/gardena-smart-gateway-mt7688-ram_defconfig
@@ -61,7 +61,7 @@ CONFIG_PHY=y
 CONFIG_POWER_DOMAIN=y
 CONFIG_RAM=y
 CONFIG_DM_RESET=y
-CONFIG_BAUDRATE=57600
+CONFIG_BAUDRATE=115200
 # CONFIG_SPL_SERIAL_PRESENT is not set
 CONFIG_SYS_NS16550=y
 CONFIG_SPI=y
