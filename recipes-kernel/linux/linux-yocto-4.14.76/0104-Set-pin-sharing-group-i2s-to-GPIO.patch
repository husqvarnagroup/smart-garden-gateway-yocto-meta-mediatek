From eeae57a2e4886fe96789aac0aa23f9c9f39d8e7c Mon Sep 17 00:00:00 2001
From: Reto Schneider <reto.schneider@husqvarnagroup.com>
Date: Mon, 12 Nov 2018 17:02:29 +0100
Subject: [PATCH] Set pin sharing group "i2s" to GPIO

And simplify specifying the others too.
---
 .../boot/dts/ralink/gardena_smart_gateway_mt7688.dts  | 19 +++----------------
 1 file changed, 3 insertions(+), 16 deletions(-)

diff --git a/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts b/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
index 93f83115aefe..7fe0b5f504d6 100644
--- a/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
+++ b/arch/mips/boot/dts/ralink/gardena_smart_gateway_mt7688.dts
@@ -26,22 +26,9 @@
 &pinctrl {
 	state_default: pinctrl0 {
 		gpio {
-			ralink,group = "gpio";
-			ralink,function = "gpio";
-		};
-
-		refclk {
-			ralink,group = "refclk";
-			ralink,function = "gpio";
-		};
-
-		spis {
-			ralink,group = "spis";
-			ralink,function = "gpio";
-		};
-
-		wdt {
-			ralink,group = "wdt";
+			ralink,group = "gpio", "i2s", "p1led_an", "p2led_an", "p3led_an", "p4led_an",
+			               "spis", "wled_an", "wdt", "sdmode", "refclk", "perst", "pwm0",
+			               "pwm1";
 			ralink,function = "gpio";
 		};
 	};
